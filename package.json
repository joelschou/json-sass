{
  "name": "json-sass-vars",
  "version": "3.1.0",
  "description": "Transform JSON into SCSS variable syntax.",
  "main": "lib/jsonSass.js",
  "bin": "bin/json-sass",
  "scripts": {
    "build": "rm -rf lib && babel ./src -d ./lib",
    "pretest": "npm run build",
    "test": "mocha --require ./lib/test-init.js lib/**/__tests__/*-test.js",
    "prepublish": "in-install || npm test"
  },
  "keywords": [
    "sass",
    "JSON"
  ],
  "author": "Andrew Clark <acdlite@me.com>",
  "license": "ISC",
  "contributors": [
    "Charlie Robbins <charlie.robbins@gmail.com>"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/acdlite/json-sass.git"
  },
  "babel": {
    "presets": [
      "babel-preset-es2015"
    ]
  },
  "dependencies": {
    "in-publish": "^2.0.0",
    "lodash.isplainobject": "^4.0.6",
    "minimist": "~1.2.0",
    "object-assign": "~4.1.1",
    "through2": "~3.0.1"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "chai": "~4.2.0",
    "mocha": "~6.1.4"
  }
}
